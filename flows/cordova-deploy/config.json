{
  "name": "cordova-deploy",
  "description": "Deploy the distribution in the folder ./www of a Cordova App",
  "steps": {
    "cordova-inquirer": {
      "app" : {
        "requires" : {
          "cells-app": "^0.6.0"
        }
      }
    },
    "gulpcordova": {
      "app" : {
        "requires" : {
          "cells-app": "^0.6.0"
        }
      },
      "inputs": {
        "type": { "cordova-inquirer": "type" },
        "environment": { "cordova-inquirer": "environment" },
        "startPoint": { "cordova-inquirer": "startPoint" },
        "cordovaApp": { "cordova-inquirer": "cordovaApp" }
      }
    }
  }
}
